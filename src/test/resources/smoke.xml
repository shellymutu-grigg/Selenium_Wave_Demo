<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!-- Error Handling Tests -->
<suite parallel="classes" name="Smoke Test Suite">
	<groups>
		<run>
			<include name="Smoke"/>
		</run>
	</groups>
	<listeners>
		<listener class-name="com.automation.test.configuration.SetupListener"/>
	</listeners>

	<test thread-count="3" parallel="classes" name="Smoke Test Suite">
    	<classes>
      		<class name="com.automation.test.processes.LoginSuccessTest"/>
    	</classes>
  	</test> 
</suite>